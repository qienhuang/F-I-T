      "labels": "PAE_and_MSA_oracles_revealed_on_query",
      "out_of_scope": [
        "PAE_as_feature",
        "MSA_as_feature"
      ]
    },
    "deploy_boundary": {
      "features": "B0_only",
      "labels": "none",
      "out_of_scope": [
        "PAE",
        "MSA"
      ]
    },
    "data_filters": {
      "min_length": 50,
      "max_length": 2500,
      "drop_na_features": true,
      "drop_na_oracles": false
    }
  },
  "oracle_costs": {
    "pae_unit_cost": 1.0,
    "msa_unit_cost": 2.0
  },
  "availability_delay_max_rounds": 0,
  "deploy_boundary": {
    "features": "B0_only",
    "labels": "none",
    "out_of_scope": [
      "PAE",
      "MSA"
    ]
  },
  "feature_whitelist": [
    "length",
    "I1_hi_conf_frac",
    "I2_mean_plddt",
    "I3_plddt_entropy",
    "C1_low_conf_frac"
  ],
  "oracle_fields": {
    "pae_value": "C2_pae_offdiag",
    "msa_depth": "msa_depth"
  },
  "thresholds": {
    "tau_pae": 1.0,
    "tau_msa_depth": 16
  },
  "holdout_role": "reporting_only",
  "universe_mode": "union",
  "policy_diagnostics": {
    "use_split": "labeled_val",
    "gap_definition": {
      "w_tpr": 1.0,
      "w_fpr": 5.0
    }
  },
  "ranking_policy_grammar": "<base>[__a<alpha>][__K<Kcap>]",
  "candidate_pool_basis_map": {
    "composite_batch_ff_uK": "uncertainty_topK",
    "composite_batch_ff_rK": "random_hash_K"
  },
  "regime_definition": {
    "labels": [
      "UNTRAINED",
      "FPR_FLOOR",
      "UNUSABLE",
      "USABLE"
    ],
    "definition": {
      "UNTRAINED": "model not trained (min labels or single class)",
      "FPR_FLOOR": "trained but fpr_floor_at_tpr_min > cap on holdout",
      "UNUSABLE": "trained, no floor, but not usable at cap (fails TPR or FPR)",
      "USABLE": "usable at cap (FPR<=cap and TPR>=tpr_min)"
    }
  },
  "delta_lag_definition": "delta = r_joint_usable - max(r_floor_resolved_pae, r_floor_resolved_msa)",
  "cov_joint_definition": "cov_joint(r) = min(TPR_pae(r), TPR_msa(r)) * 1[PAE usable at cap AND MSA usable at cap] (holdout; primary cap)",
  "E_covjump_joint_definition": "E_covjump_joint = detect_covjump(cov_joint, W_jump_rounds, delta_cov_joint)",
  "jump_type_definition": "jump_delay = r_covjump_joint - r_joint_usable; availability_driven iff jump_delay <= availability_delay_max_rounds (preregistered)"
}